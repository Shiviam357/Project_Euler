def PrimeSummation(number):
    sum_prime_numbers,factors_count=2,0

    for i in range(3,number,2):            #only odd numbers are prime besides 3 so run loop from 3 at a step of 2...odd series
        for j in range(2,i+1,1):           #prime numbers are either divisible by 1 (excluded here) or themselves so prime factors count always=1
            if (i%j==0): factors_count+=1  #count the number of factors

        if factors_count==1:                      #check for factors count and 
            sum_prime_numbers=sum_prime_numbers+i #cumulate sum of prime numbers   
        factors_count=0                           #reset the factors count for next loop
    return sum_prime_numbers                      #return the sum of prime number
