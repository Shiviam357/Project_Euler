def Factors_Nature(number):
    factors=[1] #numeric list of factors
    for i in range(2,number+1):
         if number%i==0: factors.append(i)     #check if is a factor leaving 0 remaninder and append it to the factor list
 #check for factors count (len of list) #prime numbers have only 2 factors
    print(number,"factors=>",factors,"prime:",len(factors)==2) 
    return None
